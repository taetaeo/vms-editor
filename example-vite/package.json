{
  "name": "vite-ts-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf node_modules & rm -rf package-lock.json",
    "prepare:build": "rm -rf dist",
    "dev": "vite --host",
    "build": "npm run prepare:build && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --host --port 8080"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "classnames": "^2.5.1",
    "csstype": "^3.1.3",
    "fabric": "^5.3.0",
    "fabricjs-react": "^1.2.2",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "vms-editor": "file:../package"
  },
  "devDependencies": {
    "@types/fabric": "^5.3.7",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-color": "^3.0.12",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.8",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "sass": "^1.77.8",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  }
}
